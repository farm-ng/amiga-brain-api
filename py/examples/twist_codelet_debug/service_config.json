{   "configs": [
        {
            "name": "canbus",
            "port": 6001,
            "host": "localhost",
            "log_level": "INFO",
            "subscriptions": [
                {
                    "uri": {
                        "path": "*",
                        "query": "service_name=canbus"
                    },
                    "every_n": 1
                }
            ]
        },
        {
            "name": "filter",
            "port": 20001,
            "host": "localhost",
            "log_level": "INFO",
            "subscriptions": [
                {
                    "uri": {
                        "path": "/state",
                        "query": "service_name=filter"
                    },
                    "every_n": 1
                }
            ]
        }
    ]
}
